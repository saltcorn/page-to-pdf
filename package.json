{
  "name": "@saltcorn/page-to-pdf",
  "version": "0.4.3",
  "description": "Generate PDF from page",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/markup": "^0.5.6",
    "@saltcorn/data": "^0.5.6",
    "handlebars": "^4.7.6",
    "@saltcorn/puppeteer-v17": "^17.1.4-beta.0"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": "github:saltcorn/page-to-pdf",
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
